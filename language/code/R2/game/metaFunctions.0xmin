#;
let if(condision,then,else){
	void repeat !!+condision:...codeof then();
	void repeat !+condision: ...codeof else();
};
let forEach(block,do){//v,i,a,
	#let i=0;
	repeat block..length:{
		$do(block[+i],+i,block);
		#set i+=1;
	};
};
let min(a,b){
	repeat !!(a<b):#return=a;
	repeat !!(b<a):#return=b;
};
let max(a,b){
	repeat !!(a>b):#return=a;
	repeat !!(b>a):#return=b;
};