#(){//documentation
	var:(int)->{@int};
	const:(int)->{@int};
	var:{...var[]};
	const:{...const[]};
};
#let: var(val)={
	#def: val;
	#set: constructor::{null}¬..splice(-1,1,{def this});
};
#let: const(val)={
	#def: val;
	#let: name=""...+val;
	#let: label = constructor[""...name];
	#repeat !label:{
		#set: label=this;
		#let: constructor[""...name]=label;
		#set: constructor::{@null}¬..splice(-1,1,{def this});
	};
	#set: return=label;
};