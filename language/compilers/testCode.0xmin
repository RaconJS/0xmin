let showFlags(){
	send r1 ":";
	{:block;
		jump -> block..return if !overflow;
		send r1 "O";
	};
	{:block;
		jump -> block..return if !carry;
		send r1 "C";
	};
	{:block;
		jump -> block..return if !0;
		send r1 "Z";
	};
	{:block;
		jump -> block..return if !sign;
		send r1 "S";
	};
	send r1 "\n";
};
import lib "R2/print.0xmin";
let subtract(a,b){
	r1 = 0;
	r0 = #(0xffff&a);
	print(""...+a..." - "...+b);
	r0 - (0xffff&b);
	showFlags();
};
